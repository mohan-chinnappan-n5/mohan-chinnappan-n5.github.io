{
  "manifest_version": 3,
  "name": "Bible Verse Viewer",
  "version": "1.0",
  "description": "A simple Chrome extension to view Bible verses from a CSV file.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}